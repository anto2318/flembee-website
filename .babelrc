{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@lingui/babel-preset-react"
    ],
    "plugins": [
        "macros",
        "@babel/plugin-transform-runtime",
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "true"
            }
        ]
    ]
}
